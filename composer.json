{
    "name": "yelei/aries",
    "description": "A lightweight PHP framework based on Swoole",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-swoole": "^4.8",
        "symfony/yaml": "^5.4",
        "symfony/config": "^5.4",
        "monolog/monolog": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.64",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpunit/phpunit": "^9.0",
        "swoole/ide-helper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Aries\\": "framework/",
            "App\\": "app/"
        },
        "files": [
            "framework/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
